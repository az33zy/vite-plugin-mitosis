    "mitosis",
    "jsx",
    "tsx",
    "svelte"
  ],
  "author": "az33zy",
  "license": "MIT",
  "funding": "https://github.com/sponsors/az33zy",
  "bugs": {
    "url": "https://github.com/az33zy/vite-plugin-mitosis/issues"
  },
  "homepage": "https://github.com/az33zy/vite-plugin-mitosis#readme",
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "vite": ">=2.6.0"
  },
  "dependencies": {
    "@builder.io/mitosis": "^0.5.18"
